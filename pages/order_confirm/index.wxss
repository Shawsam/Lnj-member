/**index.wxss**/
@import "../../components/myModal/index.wxss";
.flexbox { display:-webkit-box; display:-webkit-box-flex; display:box; display:-webkit-flex; display:flex; -webkit-box-pack:justify; -webkit-justify-content:space-around; }
.flexbox > .flex-item { -webkit-box-flex:1; box-flex:1; flex:1;  }
::-webkit-scrollbar { width: 0; height: 0; color: transparent;}
button:after { border:none; }

page{ height:100%; font:26rpx/40rpx "微软雅黑"; }
.container{ box-sizing:border-box;  height:100%; overflow-y:scroll;-webkit-overflow-scrolling:touch;  position:relative; background:#f3f3f3; padding-top:90rpx; padding-bottom:110rpx; }

.header{ z-index:100; position:fixed; top:0; height:50rpx; padding:20rpx; width:100%; background:#302F34; color:#FDD01B; }
.header .welcome{ font:28rpx/50rpx "微软雅黑"; height:50rpx; text-align:center;}
.header .welcome view{ display:inline-block; }
.header .welcome .name{ font-weight:bold; }
.header .welcome .dot{ margin:0 10rpx; }

.footer{ z-index:100; position:fixed; bottom:0; height:110rpx; width:100%; background:#fff; box-sizing:border-box; box-shadow:inset 0 1px 0 0 rgba(0,0,0,0.05); overflow:hidden; zoom:1; }
.footer > view{ float:left; }
.footer .total_fee{ color:#e5004f; font:30rpx/50rpx "微软雅黑"; font-weight:bold; margin-left:30rpx; margin-right:30rpx; padding-top:35rpx; vertical-align:bottom;}
.footer .total_fee .fee{ font:44rpx/50rpx "微软雅黑"; font-weight:bold; }
.footer .detailBtn{ border-radius:4rpx; border:2rpx solid #bfbfbf; color:#333; font:24rpx/36rpx "微软雅黑"; padding:0 16rpx; margin-top:40rpx; }
.footer .checkBtn{ position:absolute; border-radius:0; top:0; right:0; height:110rpx;  font:36rpx/110rpx "微软雅黑"; width:300rpx; background:#FDD01B; font-weight:bold; text-align:center; }
.footer .checkBtn.num:active{ opacity:0.8;  }



.main{ padding:30rpx 40rpx; }
.main .cont{ background:#fff; border-radius:10rpx; padding:0 40rpx; margin-bottom:30rpx; box-shadow:0 0rem 12rpx rgba(0, 0,0,0.3);}
.main .cont .con1 view{ display:inline-block; }
.right{position:absolute;top:50%;right:0; -webkit-transform: translate(0,-50%);-moz-transform:translate(0,-50%);transform: translate(0,-50%);}
.right1{position:absolute;top:0;right:0;width:100%;text-align:right;}
.con1{line-height:120rpx; position:relative;}
.con2{position:relative; line-height:120rpx;}
.cont.payCon{ padding:20rpx 40rpx;  }
.payCon .con2{ line-height:90rpx; }
.payCon .title{ color:#000; }
.payCon .con2 i{ color:#e5004f; }
.conheight{height:120rpx; font:26rpx/120rpx "微软雅黑";}
.sty1 image{ height:60rpx; width:60rpx; vertical-align:middle; margin-right:15rpx; color:#999;}
.sty1{ font:26rpx/120rpx "微软雅黑"; margin-right:60rpx; }
.sty2 image{ height:60rpx; width:60rpx; vertical-align:middle; margin-right:15rpx; color:#999;} 
.sty2{ font:26rpx/120rpx "微软雅黑"; }
.sty1.active{ color:#FC8D17; }
.sty2.active{ color:#FC8D17; }

.couponCon .right{ color:#999; }
.right .chooseItem{ display:inline-block; vertical-align:middle; color:#999; height:120rpx; line-height:120rpx; position:relative; padding-left:80rpx; }
.right .chooseItem image{ height:30rpx; width:30rpx; position:absolute; top:45rpx; left:35rpx;  }
.right .chooseItem.active{ color:#DD9D03; }

.arrodow{width:24rpx; height:16rpx; vertical-align:middle; margin-left:20rpx;}
.rote{transform:rotate(-90deg);-ms-transform:rotate(-90deg);-moz-transform:rotate(-90deg);-webkit-transform:rotate(-90deg);-o-transform:rotate(-90deg);position:relative;top:-2px;}
.togg{transform:rotate(-180deg);-ms-transform:rotate(-180deg);-moz-transform:rotate(-180deg);-webkit-transform:rotate(-180deg);-o-transform:rotate(-180deg);position:relative;top:0;}
.tota_bar{color:#ed145b; font:36rpx/90rpx "微软雅黑"; }
.payview{ display:inline-block; }
.tag{ height:50rpx; width:50rpx; vertical-align:middle; margin-right:25rpx; }
.colored{ color:#e5004f!important; }

.carditem .info1 > view{ color:#ed145b!important; }

.hide_cont{border-top: 2rpx solid hsl(0, 0%, 93%); padding:24rpx 0; background:#fff; display:none!important;}
.cont.slide .hide_cont{ display:block!important;}
.cont.slide .hide_cont .arrodow{transform:rotate(-180deg);-ms-transform:rotate(-180deg);-moz-transform:rotate(-180deg);-webkit-transform:rotate(-180deg);-o-transform:rotate(-180deg);position:relative;top:0;}
.cont .con1.slide .hide_cont{ display:block!important;}
.cont .con1.slide .hide_cont .arrodow{transform:rotate(-180deg);-ms-transform:rotate(-180deg);-moz-transform:rotate(-180deg);-webkit-transform:rotate(-180deg);-o-transform:rotate(-180deg);position:relative;top:0;}
.tipInfo{ color:#999; }
.hide_cont .coupon_change{ display:block!important; height:40px; position:relative;}
.hide_cont .ensure{display: inline-block; width:110rpx; line-height:60rpx; background:#FDD01B; font-weight:bold; float:right; text-align:center; border-radius:6rpx;  }
.hide_cont #couponInput{ display: inline-block; float:left; width:450rpx; border:2rpx solid #b5b5b5; line-height:62rpx; height:62rpx; border-radius:6rpx; padding:0 90rpx 0 15rpx; box-sizing:border-box; }
.hide_cont #couponInput input{ width:100%; height:100%; display:inline-block;  }
.couponVal{ color:#e80314; margin-left:15rpx; }
.couponTitle{ display:inline-block; position:relative; z-index:100; }
.couponTip{ font-size:20rpx;  display:block!important; color:#F0AF23; line-height:80rpx; border-top: 2rpx solid hsl(0, 0%, 93%);}
.wen{ display:inline-block; height:30rpx; width:30rpx; margin-left:10rpx; vertical-align:middle; }
.coupon_change #couponInput{ position:relative;  }
.coupon_change #couponInput .clear{ height:40rpx; width:40rpx; position:absolute; right:40rpx; top:8rpx; z-index:100;  }


.hide_cont .tips{ border-bottom:2rpx solid #eeeeee; margin-top:-13px; line-height:45px; margin-bottom:10px;}
.hide_cont .tel_change{ display:block!important; height:40px;}
.hide_cont .ensure{display: inline-block; width:110rpx; line-height:60rpx; background:#FDD01B; font-weight:bold; float:right; text-align:center; border-radius:6rpx;  }
.hide_cont #telphone{ display: inline-block; width:380rpx; border:2rpx solid #b5b5b5; line-height:60rpx; height:60rpx; border-radius:6rpx; padding-left:24rpx; vertical-align:top;}
.hide_cont #demand{ width:100%; margin-bottom:20rpx; height:200rpx; padding:8rpx 15rpx; font:24rpx/40rpx "微软雅黑"; border-radius:6rpx; box-sizing:border-box; border:2rpx solid #e8e8e8;}
.hide_cont .text{ width:100%; margin-bottom:20rpx; height:200rpx; padding:20rpx 25rpx; font:24rpx/40rpx "微软雅黑"; border-radius:6rpx; box-sizing:border-box; border:2rpx solid #e8e8e8; }

.panel_cover{position:fixed; width:100%; height:100%; background:rgba(0,0,0,0.7); top:0; left:0; z-index:105; }
.panel .detail_panel{ z-index:200; position:fixed; box-sizing:border-box; top:10%; max-height:80%; width:80%; left:10%; background:#fff; border-radius:10rpx; padding-top:90rpx;}
.panel .detail_panel .hd-title{ z-index:200; position:absolute; top:0; width:100%; height:90rpx; text-align:center; background:#FDD01B; font:30rpx/90rpx "微软雅黑"; font-weight:bold; border-radius:10rpx 10rpx 0 0; }
.panel .detail_panel .hd-title .closeDetail{ position:absolute; top:0; right:0; font:26rpx/90rpx "微软雅黑"; padding:0 30rpx;}


.panel .detail_panel .list { padding:10rpx 30rpx; max-height:700rpx; overflow-y:scroll;}
.panel .detail_panel .list .item{ overflow:hidden; zoom:1; border-bottom:1px dashed #999; padding:20rpx 10rpx; font:28rpx/40rpx "微软雅黑"; position:relative;}
.panel .detail_panel .list .package{  padding:20rpx 10rpx; border-bottom:1px dashed #999;}
.panel .detail_panel .list .package view{ display:inline-block; }
.panel .detail_panel .list .package .name{width:55%; font-weight:bold;}
.panel .detail_panel .list .package .price{width:20%; text-align:right; color:#333;}
.panel .detail_panel .list .package .num{ width:25%; text-align:center; color:#333;}

.panel .detail_panel .list .item .info1 view{ display:inline-block; font:26rpx/40rpx "微软雅黑"; vertical-align:top; }
.panel .detail_panel .list .item .info1 .name{ width:55%; font-weight:bold; }
.panel .detail_panel .list .item .info1 .name view{ font-weight:bold; }
.panel .detail_panel .list .item .info1 .price{ width:20%; text-align:right; color:#333;}
.panel .detail_panel .list .item .info1 .num{ width:25%; text-align:center; color:#333;}
.panel .detail_panel .list .item .info2{ padding-right:40%; }
.panel .detail_panel .list .item .info2 view{ display:inline-block; font:24rpx/40rpx "微软雅黑"; }
.panel .detail_panel .list .item.card .info1 .name{ font-weight:bold; }
.panel .detail_panel .list .item.card .info1 .price{ text-align:right; color:#ff6263;}
.panel .detail_panel .list .item.coupon .info1 .name{ width:75%; font-weight:bold; }
.panel .detail_panel .list .item.coupon .info1 .price{ width:25%; text-align:right; color:#ff6263;}

.panel .detail_panel .list .item .price_bar{ font:26rpx/50rpx "微软雅黑"; height:50rpx; color:#e80314; }
.panel .detail_panel .list .item .price_bar .price{ font:32rpx/50rpx "微软雅黑"; display:inline-block; color:#e80314;}
.panel .detail_panel .list .item .quantity{ height:50rpx; position:absolute; top:60rpx; right:0;}
.panel .detail_panel .list .item .quantity .plus{ display:inline-block; width:36rpx; height:36rpx; font:28rpx/36rpx "微软雅黑"; text-align:center;  vertical-align:middle; border:2rpx solid #c5c5c5; border-radius:25rpx;}
.panel .detail_panel .list .item .quantity .minus{ display:inline-block; width:36rpx; height:36rpx; font:28rpx/36rpx "微软雅黑"; text-align:center;  vertical-align:middle; border:2rpx solid #c5c5c5; border-radius:25rpx;}
.panel .detail_panel .list .item .quantity .number{ display:inline-block; width:64rpx;  font:28rpx/40rpx "微软雅黑"; text-align:center; vertical-align:middle;}
.panel .detail_panel .total{ text-align:right; padding:20rpx 40rpx; font:32rpx/40rpx "微软雅黑"; font-weight:bold;}
.panel .detail_panel .total .flag{ display:inline-block; margin:0 2rpx 0 20rpx; font:26rpx/40rpx "微软雅黑"; }
.panel .tip_panel{ z-index:200; position:fixed; box-sizing:border-box; top:10%; max-height:80%; width:80%; left:10%; background:#fff; border-radius:10rpx; padding-top:90rpx;}
.panel .tip_panel .hd-title{  z-index:200; position:absolute; top:0; width:100%; height:90rpx; text-align:center; background:#FDD01B; font:30rpx/90rpx "微软雅黑"; font-weight:bold; border-radius:10rpx 10rpx 0 0; }
.panel .tip_panel .confirmBtn{ font:30rpx/85rpx "微软雅黑"; text-align:center; font-weight:bold; border-top:2rpx solid #e8e8e8;  }
.panel .tip_panel .info{ padding:30rpx 0; }
.panel .tip_panel .info view{ font:28rpx/60rpx "微软雅黑"; text-align:center; }
.noscroll{ overflow:hidden; } 
.phoneInput{ border:2rpx solid #e8e8e8; margin-top:15rpx; height:70rpx; line-height:70rpx; }
.notice{ z-index:10000; position:fixed; height:500rpx; width:490rpx; border-radius:10rpx; overflow:hidden; left:50%; top:50%; transform:translate(-50%,-50%); background:#fff; }
.notice .img{ height:100%; width:100%; }
.notice .btn{ position:fixed; z-index:10000; right:0; top:0; height:40rpx; width:40rpx; padding:20rpx; }


.wx-switch-input{width:45px!important;height:24px !important;}
.wx-switch-input::before{width:45px !important; height:24px !important;}
.wx-switch-input::after{width:22px !important; height:22px !important;}

.panel .coupon_panel{ z-index:200; position:fixed; box-sizing:border-box; top:10%; width:80%; left:10%; background:#fff; border-radius:10rpx; padding-top:90rpx;}
.panel .coupon_panel .hd-title{  z-index:200; position:absolute; top:0; width:100%; height:90rpx; text-align:center; background:#FDD01B; font:30rpx/100rpx "微软雅黑"; color:#fff; font-weight:bold; border-radius:10rpx 10rpx 0 0; }
.panel .coupon_panel .confirmBtn{ font:30rpx/85rpx "微软雅黑"; text-align:center; font-weight:bold; background:#FDD01B; border-radius:45rpx; width:90%; margin:15rpx auto 45rpx auto; }
.panel .coupon_panel .info{ padding:30rpx; max-height:700rpx; overflow-y:scroll; }
.panel .coupon_panel .info view{ font:28rpx/50rpx "微软雅黑"; text-align:left; color:#666; }
.panel .coupon_panel .info view.tit{ font:28rpx/55rpx "微软雅黑"; font-weight:bold; color:#000; }
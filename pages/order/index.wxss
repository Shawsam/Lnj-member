@import "../../components/myModal/index.wxss";
.flexbox { display:-webkit-box; display:-webkit-box-flex; display:box; display:-webkit-flex; display:flex; -webkit-box-pack:justify; -webkit-justify-content:space-around; }
.flexbox > .flex-item { -webkit-box-flex:1; box-flex:1; flex:1;  }
.words_line2 { display: -webkit-box;display: box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;overflow: hidden; }
::-webkit-scrollbar { width: 0; height: 0; color: transparent;}

page{ font:26rpx/40rpx "微软雅黑"; height:100%;}
.container{ height:100%;  padding-bottom:55px; box-sizing:border-box; position:relative; z-index:100;}
.banner{ height:140px; width:100%; font-size:0; }
.banner.overHeight{ height:0; width:0; }
.banner image{ height:140px; width:100%; display:block; }
.header{ padding:0 10px; height:45px; background:#302F34; color:#FDD01B; text-align:left; z-index:1000; width:100%; box-sizing:border-box; }
.header .welcome{ line-height:45px;  }
.header .welcome view{ display:inline-block; }
.header .welcome .name{ font-weight:bold; }
.header .welcome .dot{ margin:0 5px; }
.header .welcome .meal{ float:right; }
.header .welcome .meal .icon{ display:inline-block; height:30rpx; width:30rpx; vertical-align:middle; margin-top:-5rpx; margin-right:8rpx; }


.footer{ z-index:105; position:fixed; bottom:0; height:55px; width:100%; background:#fff; box-sizing:border-box; box-shadow:inset 0 1px 0 0 rgba(0,0,0,0.05); overflow:hidden; zoom:1; }
.footer > view{ float:left; }
.footer .cart{ width:70px; text-align:center; position:relative; }
.footer .cart .cart_img{ display:inline-block; height:36px; width:42px; padding-top:12px; }
.footer .cart .item_num{ position:absolute;  font:20rpx/18px "微软雅黑"; right:6px; top:10px; background:#e5004f; color:#fff; line-height:20px; border-radius:18rpx; min-width:18px; }
.footer .total_fee{ color:#e5004f; font:30rpx/25px "微软雅黑"; font-weight:bold; margin-right:30rpx; padding-top:17.5px; vertical-align:bottom;}
.footer .total_fee .fee{ font:48rpx/25px "微软雅黑"; font-weight:bold; }
.footer .detailBtn{ border-radius:4rpx; border:1px solid #bfbfbf; color:#333; font:24rpx/18px "微软雅黑"; padding:0 8px; margin-top:20px; }
.footer .checkBtn{ position:absolute; top:0; right:0; height:55px;  font:36rpx/55px "微软雅黑"; width:300rpx; font-weight:bold; text-align:center;}
.footer .checkBtn.none{ background:#CFCFCF; }
.footer .checkBtn.num{background:#FDD01B; }
.footer .detailBtn:active{ opacity:0.8;  }
.footer .checkBtn.num:active{ opacity:0.8;  }

.main{ height:100%; }

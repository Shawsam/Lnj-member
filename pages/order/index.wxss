/**index.wxss**/
@import "../../components/myModal/index.wxss";
.flexbox { display:-webkit-box; display:-webkit-box-flex; display:box; display:-webkit-flex; display:flex; -webkit-box-pack:justify; -webkit-justify-content:space-around; }
.flexbox > .flex-item { -webkit-box-flex:1; box-flex:1; flex:1;  }
.words_line2 { display: -webkit-box;display: box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;overflow: hidden; }
::-webkit-scrollbar { width: 0; height: 0; color: transparent;}

page{ height:100%; font:26rpx/40rpx "微软雅黑"; }
.container{ box-sizing:border-box;  height:100%; overflow-y:scroll;-webkit-overflow-scrolling:touch; box-sizing:border-box; position:relative; }
.container{ padding-top:90rpx; padding-bottom:110rpx; }

.header{ z-index:100; position:fixed; top:0; height:50rpx; padding:20rpx; width:100%; background:#302F34; color:#FDD01B; }
.header .welcome{ font:28rpx/50rpx "微软雅黑"; height:50rpx; text-align:center;}
.header .welcome view{ display:inline-block; }
.header .welcome .name{ font-weight:bold; }
.header .welcome .dot{ margin:0 10rpx; }

.footer{ z-index:100; position:fixed; bottom:0; height:110rpx; width:100%; background:#fff; box-sizing:border-box; box-shadow:inset 0 1px 0 0 rgba(0,0,0,0.05); overflow:hidden; zoom:1; }
.footer > view{ float:left; }
.footer .cart{ width:140rpx; text-align:center; position:relative; }
.footer .cart .cart_img{ display:inline-block; height:72rpx; width:84rpx; padding-top:24rpx; }
.footer .cart .item_num{ position:absolute;  font:20rpx/36rpx "微软雅黑"; right:12rpx; top:20rpx; background:#e5004f; color:#fff; line-height:40rpx; border-radius:18rpx; min-width:36rpx; }
.footer .total_fee{ color:#e5004f; font:30rpx/50rpx "微软雅黑"; font-weight:bold; margin-right:30rpx; padding-top:35rpx; vertical-align:bottom;}
.footer .total_fee .fee{ font:48rpx/50rpx "微软雅黑"; font-weight:bold; }
.footer .detailBtn{ border-radius:4rpx; border:1px solid #bfbfbf; color:#333; font:24rpx/36rpx "微软雅黑"; padding:0 16rpx; margin-top:40rpx; }
.footer .checkBtn{ position:absolute; top:0; right:0; height:110rpx;  font:36rpx/110rpx "微软雅黑"; width:300rpx; font-weight:bold; text-align:center;}
.footer .checkBtn.none{ background:#CFCFCF; }
.footer .checkBtn.num{background:#FDD01B; }
.footer .detailBtn:active{ opacity:0.8;  }
.footer .checkBtn.num:active{ opacity:0.8;  }


.main{ overflow:hidden; zoom:1; height:100%; }
.main .tab_list{  z-index:100; position:relative; margin-right:-2rpx; float:left; width:23%; overflow-y:scroll;-webkit-overflow-scrolling:touch; height:100%; background:#FDD01B; box-sizing:border-box; padding-left:18rpx; padding-bottom:5px; }
.main .tab_list .tab{ overflow:hidden; height:186rpx; width:100%; font:28rpx/40rpx "微软雅黑"; font-weight:bold; text-align:center; margin-bottom:-10rpx;}
.main .tab_list .tab > image{ height:70rpx; width:70rpx; vertical-align:middle; margin-top:38rpx; }
.main .tab_list .tab.active{ background:url(data:image/png;base64,)no-repeat; background-size:cover;  }

.main .item_list{  z-index:101; position:relative; float:left; width:77%; overflow-y:scroll;-webkit-overflow-scrolling:touch; height:100%; background:#fff; box-sizing:border-box; padding-top:80rpx;}
.main .item_list .title{ color:#000; padding:20rpx 50rpx 20rpx 20rpx; font:36rpx/40rpx "微软雅黑"; height:40rpx; width:100%; font-weight:bold; position:fixed; top:90rpx; z-index:1000; display:block; background:#fff;}
.main .item_list .item{ overflow:hidden; padding:30rpx 10rpx; height:230rpx; position:relative; padding-left:270rpx; }

.main .item_list .item .shade{ position:absolute; left:0; top:0; height:100%; width:100%; z-index:100; background:rgba(0,0,0,0.1); display:none; }
.main .item_list .item .shade.show{ display:block; }
.main .item_list .item > image{ height:230rpx; width:230rpx; border-radius:10rpx; position:absolute; left:20rpx; }
.main .item_list .item .name{ font:36rpx/40rpx "微软雅黑"; font-weight:bold; padding:10rpx 0 6rpx 0;}
.main .item_list .item .desp{ font:28rpx/40rpx "微软雅黑"; color:#999; overflow: hidden; text-overflow:ellipsis; white-space: nowrap; }
.main .item_list .item .stock_state{ font:26rpx/40rpx "微软雅黑"; color:#ff6263; }

.main .item_list .item .price_bar{ font:26rpx/50rpx "微软雅黑"; height:50rpx; color:#e80314; position:absolute; bottom:30rpx; left:260rpx; }
.main .item_list .item .price_bar .price{ font:30rpx/50rpx "微软雅黑"; display:inline-block; color:#e80314;}
.main .item_list .item .quantity{ height:50rpx; position:absolute; bottom:35rpx; right:15rpx;}
.main .item_list .item .quantity .plus{ display:inline-block; width:50rpx; height:50rpx; text-align:center; vertical-align:middle;}
.main .item_list .item .quantity .minus{ display:inline-block; width:50rpx; height:50rpx; text-align:center; vertical-align:middle; }
.main .item_list .item .quantity .number{ display:inline-block; width:60rpx;  font:28rpx/50rpx "微软雅黑"; text-align:center; vertical-align:middle;}
.main .item_list .item .quantity .plus:active image{ width:60rpx; height:60rpx; margin:-5rpx;}
.main .item_list .item .quantity .minus:active image{ width:60rpx; height:60rpx; margin:-5rpx;}
.main .item_list .item .quantity .plus image{ width:54rpx; height:54rpx; margin:-2rpx;}
.main .item_list .item .quantity .minus image{ width:54rpx; height:54rpx; margin:-2rpx;}

.main .item_list .emptyItem{ text-align:center; padding:50rpx; color:#999; }


.panel_cover{position:fixed; width:100%; height:100%; background:rgba(0,0,0,0.7); top:0; left:0; z-index:105; }
.panel .detail_panel{ z-index:200; position:fixed; box-sizing:border-box; top:10%; max-height:80%; width:80%; left:10%; background:#fff; border-radius:10rpx; padding-top:90rpx;}
.panel .detail_panel .hd-title{ z-index:200; position:absolute; top:0; width:100%; height:90rpx; text-align:center; background:#FDD01B; font:30rpx/90rpx "微软雅黑"; font-weight:bold; border-radius:10rpx 10rpx 0 0; }
.panel .detail_panel .hd-title .cartEmpty{ position:absolute; top:0; left:0; font:26rpx/90rpx "微软雅黑"; padding:0 30rpx;}
.panel .detail_panel .hd-title .closeDetail{ position:absolute; top:0; right:0; font:26rpx/90rpx "微软雅黑"; padding:0 30rpx;}

.panel .detail_panel .list { padding:10rpx 30rpx; max-height:700rpx; overflow-y:scroll; }
.panel .detail_panel .list .item{ overflow:hidden; zoom:1; border-bottom:1px dashed #999; padding:20rpx 10rpx; padding-right:160rpx; font:28rpx/40rpx "微软雅黑"; position:relative;}

.panel .detail_panel .list .item .info1 view{ display:inline-block; }
.panel .detail_panel .list .item .info1 .name{ font:26rpx/40rpx "微软雅黑"; font-weight:bold; }
.panel .detail_panel .list .item .info2 view{ display:inline-block; }
.panel .detail_panel .list .item .info2 view{ font:24rpx/40rpx "微软雅黑"; }

.panel .detail_panel .list .item .price_bar{ font:26rpx/50rpx "微软雅黑"; height:50rpx; color:#e80314; }
.panel .detail_panel .list .item .price_bar .price{ font:32rpx/50rpx "微软雅黑"; display:inline-block; color:#e80314;}
.panel .detail_panel .list .item .quantity{ height:50rpx; position:absolute; top:60rpx; right:4rpx;}
.panel .detail_panel .list .item .quantity .plus{ display:inline-block; width:50rpx; height:50rpx; text-align:center; vertical-align:middle;}
.panel .detail_panel .list .item .quantity .minus{ display:inline-block; width:50rpx; height:50rpx;text-align:center; vertical-align:middle;}
.panel .detail_panel .list .item .quantity .number{ display:inline-block; width:70rpx;  font:28rpx/50rpx "微软雅黑"; text-align:center; vertical-align:middle;}
.panel .detail_panel .list .item .quantity .plus:active{opacity:0.8}
.panel .detail_panel .list .item .quantity .minus:active{opacity:0.8}

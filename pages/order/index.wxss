.panel .detail_panel .list .item .quantity .plus:active image{ width:60rpx; height:60rpx; margin:-5rpx;}
.panel .detail_panel .list .item .quantity .minus:active image{ width:60rpx; height:60rpx; margin:-5rpx;}
.panel .detail_panel .list .item .quantity .plus image{ width:54rpx; height:54rpx; margin:-2rpx;}
.panel .detail_panel .list .item .quantity .minus image{ width:54rpx; height:54rpx; margin:-2rpx;}

.panel .detail_panel .total{ text-align:right; padding:20rpx 40rpx; font:32rpx/40rpx "微软雅黑"; font-weight:bold;}
.panel .detail_panel .total .flag{ display:inline-block; margin:0 2rpx 0 20rpx; font:26rpx/40rpx "微软雅黑"; }

.panel .choose_panel{ z-index:200; position:fixed; box-sizing:border-box; top:10%; max-height:80%; width:80%; left:10%; background:#fff; overflow-y:scroll; border-radius:10rpx; }
.panel .choose_panel{ padding:0 30rpx; padding-bottom:120rpx; padding-top:100rpx;}

.panel .choose_panel .hd-title{ z-index:200; position:fixed; top:10%; margin-left:-30rpx; box-sizing:border-box; width:80%;  height:100rpx; padding:10rpx 30rpx; }
.panel .choose_panel .hd-title .name{ display:inline-block; font:36rpx/90rpx "微软雅黑"; font-weight:bold;}
.panel .choose_panel .hd-title .cancelBtn{ float:right; font:26rpx/90rpx "微软雅黑"; padding:0 20rpx; }
.panel .choose_panel .list{ max-height:700rpx;  border-top:1px solid #e8e8e8;}
.panel .choose_panel .list .item .item_name{ font:32rpx/50rpx "微软雅黑"; font-weight:bold; margin:16rpx 0 20rpx 0;   }
.panel .choose_panel .list .item .item_name .desp{ margin-left:6rpx; font:30rpx/50rpx "微软雅黑"; display:inline-block; }
.panel .choose_panel .list .item .item_info{ font:28rpx/50rpx "微软雅黑"; border-bottom:1px dashed #e8e8e8; padding:0 0 20rpx 0;}
.panel .choose_panel .list .item .item_info> view{ display:inline-block; border:1px solid #eee; padding:0 10rpx; box-sizing:border-box; text-align:center; border-radius:10rpx; margin:0 28rpx 18rpx 0;}
.panel .choose_panel .list .item .item_info> view.disabled{ background:#eee!important; border:1px solid #eee!important; }
.panel .choose_panel .list .item .item_info> view.active{ background:#FDD01B; border:1px solid #FDD01B; }
.panel .choose_panel .confirmBtn{  font:34rpx/100rpx "微软雅黑"; font-weight:bold; height:100rpx; width:100%; position:absolute; bottom:0; right:0;  background:#FDD01B; text-align:center; }
.panel .choose_panel .confirmBtn.disabled{ background:#ccc!important; border:1px solid #ccc!important; }
.panel .choose_panel .confirmBtn:active{ opacity:0.8; }

.panel .info_panel{ z-index:200; position:fixed; overflow:hidden; box-sizing:border-box; top:120rpx; width:600rpx; left:50%; margin-left:-300rpx; background:#fff; border-radius:10rpx; }
.closeInfo{ position:absolute; top:0; right:0; font:26rpx/90rpx "微软雅黑"; padding:0 30rpx;}
.panel .info_panel image{ width:600rpx; height:600rpx; }
.panel .info_panel .name{ font:36rpx/50rpx "微软雅黑"; padding:15rpx 30rpx 0 30rpx; font-weight:bold; }
.panel .info_panel .desp{ font:24rpx/40rpx "微软雅黑"; padding:15rpx 30rpx; color:#999; }
.panel .info_panel .time{ font:24rpx/40rpx "微软雅黑"; padding:5rpx 30rpx; color:#FF843F; }
.panel .info_panel .handle{ padding:15rpx 30rpx 30rpx 30rpx; position:relative; }
.panel .info_panel .handle view{ display:inline-block; }
.panel .info_panel .handle .price_bar{ font:26rpx/50rpx "微软雅黑"; height:50rpx; color:#e80314; }
.panel .info_panel .handle .price_bar .price{ font:40rpx/50rpx "微软雅黑"; display:inline-block; color:#e80314;}
.panel .info_panel .handle .quantity{ float:right; margin-right:40rpx;}
.panel .info_panel .handle .quantity .plus{ display:inline-block; width:50rpx; height:50rpx; text-align:center; vertical-align:middle;}
.panel .info_panel .handle .quantity .minus{ display:inline-block; width:50rpx; height:50rpx; text-align:center; vertical-align:middle; }
.panel .info_panel .handle .quantity .number{ display:inline-block; width:60rpx;  font:30rpx/50rpx "微软雅黑"; text-align:center; vertical-align:middle;}
.panel .info_panel .handle .quantity .plus:active{opacity:0.8}
.panel .info_panel .handle .quantity .minus:active{opacity:0.8}
